<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system1="clr-namespace:System;assembly=mscorlib">
    <system1:String x:Key="AppName">WindowResizer</system1:String>
    <system1:String x:Key="UpdateMessage">Do you want update?</system1:String>

    <!-- Hotkeys page -->
    <system1:String x:Key="SaveShortcut">Save shortcut</system1:String>
    <system1:String x:Key="SaveShortcutDesc">Save window position and size</system1:String>
    <system1:String x:Key="RestoreShortcut">Restore shortcut</system1:String>
    <system1:String x:Key="RestoreShortcutDesc">Restore window position and size</system1:String>
    <system1:String x:Key="RestoreAllShortcut">Restore All shortcut</system1:String>
    <system1:String x:Key="RestoreAllShortcutDesc">Restore all saved windows position and size</system1:String>

    <!-- About page -->
    <system1:String x:Key="CurrentVersion">Current Version</system1:String>
    <system1:String x:Key="CheckUpdate">Check Update</system1:String>
    <system1:String x:Key="CurrentRepo">source code repository</system1:String>
    <system1:String x:Key="GoToRepo">Go</system1:String>
</ResourceDictionary>

